<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ynjh.personal.dao.userã€‚UserMapper">
    <insert id="addUser" parameterType="User" >
        insert into user(userId,userPassword,userName,userGender,userEmail,userRealName,userIDCard,userIDImg,userCreateDate,userHeadImgPath,userBirthday) 
        values(#{userId},#{userPassword},#{userName},#{userGender},#{userEmail},#{userRealName},#{userIDCard},#{userIDImg},#{userCreateDate},#{userHeadImgPath},#{userBirthday})
    </insert>
    <update id="updateUserOther" parameterType="Integer">
        update user set userHeadImgPath=#{userHeadImgPath},userPassword=#{userPassword},userGender=#{userGender},userEmail=#{userEmail},userBirthday=#{userBirthday},userName=#{userName} where userId=#{userId}
    </update>
    <update id="updateUserIDCord" parameterType="Integer">
        update user set userRealName=#{userRealName},userIDCard=#{userIDCard},userIDImg=#{userIDImg} where userId=#{userId}
    </update>
    <select id="loginByUserIdAndUserPassword"  resultType="User">
        select * from  user where userId=#{userId} and userPassword=#{userPassword}
    </select>
	
</mapper>